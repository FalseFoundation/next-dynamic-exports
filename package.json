{
	"name": "@falsefoundation/next-dynamic-exports",
	"version": "0.1.7",
	"description": "Adds dynamic route handling to Next.js static exports.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsc",
		"format": "prettier --write ./src",
		"dev": "nodemon --watch src --ext js,json,ts --exec 'tsc'"
