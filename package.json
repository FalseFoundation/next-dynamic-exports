		"README.md",
		"package.json"
	],
	"keywords": [
		"nextjs",
		"static",
		"dynamic",
		"routes",
		"exports",
		"ssg",
		"ssr",
		"next-static",
		"next-dynamic",
		"params",
		"next-static-params"
	],
	"author": "zdenham",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/FalseFoundation/next-dynamic-exports.git"
	},
	"bugs": {
		"url": "https://github.com/FalseFoundation/next-dynamic-exports/issues"
	},
	"peerDependencies": {
		"next": "^15.2.0",
		"react": "^19"
	},
	"bin": {
		"next-dynamic-exports": "dist/cli/index.js"
	},
	"devDependencies": {
		"@types/node": "^22.13.5",
		"@types/react": "^19",
		"next": "^15.2.0",
		"nodemon": "^3.1.9",
		"prettier": "^3.5.2",
		"react": "^19",
		"typescript": "^5.7.3"
